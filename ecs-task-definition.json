{
  "executionRoleArn": "arn:aws:iam::195275674689:role/ecsTaskExecutionRole",
  "family": "oscars-project-family",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024", 
  "containerDefinitions": [
    {
      "name": "flask-app",
      "image": "195275674689.dkr.ecr.us-east-2.amazonaws.com/teacher-student-hub:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ]
    },
    {
      "name": "nginx-proxy",
      "image": "195275674689.dkr.ecr.us-east-2.amazonaws.com/nginx-proxy:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}


